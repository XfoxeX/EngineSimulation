<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

  <appSettings>
    <!-- Motor moment of inertia -->
    <add key="I" value="10"/>
    <!-- Torque -->
    <add key="M" value="20, 75, 100, 105, 75, 0"/>
    <!-- Rotation speed -->
    <add key="V" value="0, 75, 150, 200, 250, 300"/>
    <!-- Superheating temperature -->
    <add key="T_max" value="300"/>
    <!-- Coef of dependence of the heating rate on the torque -->
    <add key="H_m" value="0,01"/>
    <!-- Coef of dependence of the heating rate on the rotation speed -->
    <add key="H_v" value="0,0001"/>
    <!-- Coef of dependence of the cooling rate on the engine and environment temp -->
    <add key="C" value="0,1"/>
  </appSettings>
</configuration>